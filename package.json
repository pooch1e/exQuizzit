{
  "name": "exquizzit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "NODE_ENV=test jest",
    "seed": "ts-node prisma/seed.ts",
    "seed:test": "NODE_ENV=test ts-node prisma/seed.ts",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:db:push": "dotenv -e .env.test -- npx prisma db push",
    "test:db:migrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:db:reset": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test:db:studio": "dotenv -e .env.test -- npx prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@supabase/supabase-js": "^2.50.3",
    "dotenv": "^17.0.1",
    "next": "15.3.4",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
